        position: absolute;
        right: -1px;
        background-color: #eeeeee;
        color: #999999; }
    .warp form .consent {
      margin-top: 0.65rem;
      line-height: 1.2;
      font-size: 0.25rem;
      color: #666666;
      padding-left: 1rem; }
      .warp form .consent input {
        width: 0.3rem;
        height: 0.3rem;
        float: left;
        margin-right: 0.1rem; }
    .warp form .login {
      margin-top: 1.15rem; }
      .warp form .login .login_c {
        width: 100%;
        height: 0.88rem;
        border: 1px solid #eeeeee;
        border-radius: 2px;
        background-color: #e77fc3;
        color: #fff; }
    .warp form .tourist {
      margin-top: 0.28rem; }
      .warp form .tourist .tourist_c {
        width: 100%;
        height: 0.88rem;
        border: 1px solid #eeeeee;
        border-radius: 2px;
        background-color: #ffdbf3;
        color: #e77fc3; }

