body {
  background-image: linear-gradient(to bottom, #fff, #ffc6eb); }

.warp {
  width: 100%;
  height: 100%;
  overflow: hidden; }
  .warp header {
    height: 3.65rem;
    border: 1px solid #e5e5e5; }
  .warp form {
    padding: 1.44rem 0.32rem;
    box-sizing: border-box; }
    .warp form input {
      width: 100%;
      height: 0.88rem;
      border: 1px solid #e5e5e5; }
    .warp form .photo .phone_prefix {
      width: 15%;
      height: 0.88rem;
      line-height: 0.88rem;
      background-color: #ffffff;
      border: 1px solid #e5e5e5;
      border-right: none;
      border-top-left-radius: 0.05rem;
      border-bottom-left-radius: 0.05rem;
      text-align: center; }
    .warp form .photo .mobile {
      width: 84%;
      height: 0.88rem;
      line-height: 0.88rem;
      border: 1px solid #e5e5e5;
      border-left: none;
      border-top-right-radius: 0.05rem;
      border-bottom-right-radius: 0.05rem;
      text-indent: 0; }
    .warp form .note {
      margin-top: 0.38rem;
      position: relative; }
      .warp form .note input {
        /* margin-top: 0.4rem; */
        text-indent: 0.2rem;
        border-right: 0;
        border-top-left-radius: 2px;
        border-bottom-left-radius: 2px; }
      .warp form .note button {
        height: 0.91rem;
